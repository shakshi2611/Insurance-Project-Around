{
  "name": "insurance",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server1": "json-server --watch db1.json --port 5000",
    "server2": "json-server --watch db2.json --port 5001",
    "start-servers": "run-p server1 server2",
    "dev-full": "run-p dev start-servers",
    "clear-db": "rimraf ./db2.json && echo {\"insuranceFiles\": [], \"brokerFiles\": []} > ./db2.json",
    "start": "npm run clear-db && react-scripts start",
    "start-dev": "npm run clear-db && npm run dev-full"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.2",
    "axios": "^1.7.7",
    "docx": "^9.0.0",
    "framer-motion": "^11.11.1",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.447.0",
    "mammoth": "^1.8.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.7.76",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.9",
    "react-router-dom": "^6.26.2",
    "react-toastify": "^10.0.5",
    "recharts": "^2.12.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "json-server": "^1.0.0-beta.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.13",
    "vite": "^5.4.8"
  }
